//Right state
defaultFieldValues
(
    volScalarFieldValue T       308.15
    volScalarFieldValue p       1.0e+5
    volScalarFieldValue air     0
    volScalarFieldValue water   1
    volVectorFieldValue U       (0 0 0)
);

//Left state
regions
(
    boxToCell
    {
        box ( -5 -1 -1 ) ( 0 1  1 );
        fieldValues
        (
            volScalarFieldValue p     1.01e+5
            volScalarFieldValue air   1
            volScalarFieldValue water 0
        );
    }
